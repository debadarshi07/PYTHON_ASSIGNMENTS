    "import numpy as np\n",
    "\n",
    "arr = np.array([2 ** x for x in range(6)]).reshape([2, 3])\n",
    "print(f\"Original array: \\n{arr}\\n\")\n",
    "\n",
    "print(f\"Flatten: \\n{arr.flatten()}\\n\")\n",
    "print(f\"Original array: \\n{arr}\\n\\n\")\n",
    "\n",
    "print(f\"Ravel: \\n{arr.ravel()}\\n\")\n",
    "print(f\"Original array: \\n{arr}\")"
   ]
  }
