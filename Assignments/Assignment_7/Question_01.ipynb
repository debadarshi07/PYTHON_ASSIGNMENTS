   "source": [
    "def replace(string):\n",
    "    result = string[0]\n",
    "    for i in range(1, len(string)):\n",
    "        if string[i] == string[i - 1]:\n",
    "            result += '*'\n",
    "        else:\n",
    "            result += string[i]\n",
    "    return result"
   ]
  },
